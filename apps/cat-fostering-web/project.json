{
  "name": "cat-fostering-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/cat-fostering-web/src",
  "tags": ["type:app", "platform:angular"],
  "namedInputs": {
    "dockerFiles": ["{projectRoot}/Dockerfile", "{projectRoot}/nginx.conf"]
  },
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "inputs": [
        "production",
        "^production",
        { "env": "CAT_FOSTERING_API_URL" },
        { "env": "ORY_KRATOS_PUBLIC_URL" },
        { "env": "NODE_ENV" }
      ],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/cat-fostering-web",
        "index": "apps/cat-fostering-web/src/index.html",
        "main": "apps/cat-fostering-web/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/cat-fostering-web/tsconfig.app.json",
        "assets": [
          "apps/cat-fostering-web/src/favicon.ico",
          "apps/cat-fostering-web/src/assets"
        ],
        "customWebpackConfig": {
          "path": "apps/cat-fostering-web/webpack.config.js"
        },
        "styles": [
          "@angular/material/prebuilt-themes/deeppurple-amber.css",
          "apps/cat-fostering-web/src/styles.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "50kb",
              "maximumError": "200kb"
            }
          ],
          "outputHashing": "all"
        },
        "staging": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "50kb",
              "maximumError": "200kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "cat-fostering-web:build:production"
        },
        "staging": {
          "buildTarget": "cat-fostering-web:build:staging",
          "host": "127.0.0.1",
          "port": 4200
        },
        "development": {
          "buildTarget": "cat-fostering-web:build:development",
          "host": "127.0.0.1",
          "port": 4200
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@nx/angular:extract-i18n",
      "options": {
        "buildTarget": "cat-fostering-web:build"
      }
    },
    "extract-openapi-spec": {
      "executor": "nx:run-commands",
      "cache": true,
      "inputs": ["{workspaceRoot}/apps/cat-fostering-api/openapi.json"],
      "outputs": ["{workspaceRoot}/libs/ng-shared/data-access/src/lib"],
      "options": {
        "commands": [
          "npx openapi-generator-cli generate -i apps/cat-fostering-api/openapi.json -g typescript-angular -o libs/ng-shared/data-access/src/lib --skip-validate-spec",
          "nx run ng-data-access:lint --fix",
          "nx format --projects ng-data-access"
        ],
        "parallel": false
      }
    },
    "container": {
      "executor": "@nx-tools/nx-container:build",
      "inputs": [
        "dockerFiles",
        { "dependentTasksOutputFiles": "**/dist/**/*", "transitive": true }
      ],
      "cache": true,
      "dependsOn": ["build"],
      "options": {
        "file": "apps/cat-fostering-web/Dockerfile",
        "platforms": ["linux/amd64", "linux/arm64"],
        "push": true,
        "tags": ["getlarge/cat-fostering-web:latest"],
        "build-args": ["APP_NAME=cat-fostering-web"]
      }
    }
  }
}
